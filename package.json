{
    "name": "vue3-ssr-blog",
    "version": "1.1.0",
    "license": "MIT",
    "private": true,
    "scripts": {
        "build": "yarn build:client && yarn build:server",
        "build:client": "vite build --ssrManifest --outDir dist/client",
        "build:server": "vite build --ssr src/entry-server --outDir dist/server",
        "dev": "node server-dev.js",
        "serve": "node server-prod.js",
        "check-ts-errors": "yarn tsc --noEmit && yarn vti diagnostics",
        "lint": "eslint \"src/**/*.{js,ts,vue}\"",
        "test": "yarn check-ts-errors && yarn lint",
        "perf": "yarn build && clinic doctor --on-port=\"sleep 5 && autocannon http://localhost:8080 -a 500 -c 50 && sleep 5 && autocannon http://localhost:8080 -a 10 -c 2 && sleep 5\" -- node server-prod.js",
        "prepare": "husky install"
    },
    "dependencies": {
        "@harlem/core": "^2.0.0",
        "@harlem/plugin-ssr": "^2.0.0",
        "@vue/server-renderer": "^3.2.29",
        "clinic": "^11.0.0",
        "cross-fetch": "^3.1.4",
        "graphql": "^16.2.0",
        "insane": "^2.6.2",
        "js-cookie": "^2.2.1",
        "koa": "^2.13.1",
        "koa-connect": "^2.1.0",
        "koa-send": "^5.0.1",
        "marked": "^2.0.3",
        "tslint": "^6.1.3",
        "villus": "^1.0.1",
        "vue": "^3.2.29",
        "vue-router": "^4.0.12"
    },
    "devDependencies": {
        "@babel/core": "^7.13.16",
        "@types/js-cookie": "^2.2.6",
        "@types/koa-send": "^4.1.2",
        "@types/marked": "^2.0.2",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "@vitejs/plugin-vue": "^1.6.2",
        "@vue/compiler-sfc": "^3.2.29",
        "eslint": "^7.25.0",
        "eslint-config-standard-with-typescript": "^20.0.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-vue": "^7.9.0",
        "husky": "^6.0.0",
        "lint-staged": "^10.5.4",
        "rollup-plugin-analyzer": "^4.0.0",
        "sass": "^1.49.8",
        "typescript": "^4.4.4",
        "vite": "^2.5.6",
        "vite-plugin-windicss": "^1.7.0",
        "vti": "^0.1.1",
        "windicss": "^3.4.3"
    }
}